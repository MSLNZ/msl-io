{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en-GB",
    "languageId": "python",
    "useGitignore": true,
    "dictionaries": [
        "!aws",
        "!companies",
        "!cryptocurrencies",
        "!web-services"
    ],
    "cache": {
        "cacheFormat": "universal",
        "cacheLocation": ".cache/.cspellcache",
        "cacheStrategy": "content",
        "useCache": true
    },
    "ignorePaths": [
        "cspell.json",
        "mkdocs.yml",
        "pyproject.toml",
        "src/msl/io/readers/_xlrd/*"
    ],
    "words": [
        "autouse",
        "BIPM",
        "birthtime",
        "chunksize",
        "dset",
        "gdrive",
        "geteuid",
        "googleapiclient",
        "gsheets",
        "levelname",
        "ndarray",
        "NOCLOSEPROCESS",
        "officedocument",
        "openxmlformats",
        "pycon",
        "resumable",
        "ripemd",
        "runas",
        "setflags",
        "spreadsheetml"
    ]
}
